select * from Endereco;

# Contar
select count(*) from endereco;

# Procurar
SELECT * FROM endereco WHERE distrito ILIKE '%Bela vista%';

DROP TABLE endereco;

CREATE TABLE endereco (
    zip_code VARCHAR(8) PRIMARY KEY,
    rua VARCHAR(255),
    distrito VARCHAR(255),
    cidade VARCHAR(255),
    estado VARCHAR(255)
);